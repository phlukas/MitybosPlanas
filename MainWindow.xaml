<Window x:Class="MitybosPlanas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MitybosPlanas"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800" SizeToContent="Height">
    <Grid Margin="0,4,0,-6" Height="800">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="523*"/>
            <ColumnDefinition Width="472*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="button" Content="Generuoti" HorizontalAlignment="Left" Height="30" Margin="384,728,0,0" VerticalAlignment="Top" Width="80" Click="button_Click" Cursor="" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Height="20" Margin="76,67,0,0" VerticalAlignment="Top" Width="150" AutomationProperties.HelpText="Abc" RenderTransformOrigin="0.49,2.352"/>
        <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" Margin="76,134,0,0" VerticalAlignment="Top" Width="150" Height="20"/>
        <Label x:Name="label" Content="Pirmadienis, Antradienis" HorizontalAlignment="Left" Height="30" Margin="80,34,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="label1" Content="Trečiadienis, Ketvirtadienis" HorizontalAlignment="Left" Height="30" Margin="80,102,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="label2" Content="Penktadienis, Šeštadienis" HorizontalAlignment="Left" Height="30" Margin="80,165,0,0" VerticalAlignment="Top" Width="150"/>
        <ComboBox x:Name="comboBox2" HorizontalAlignment="Left" Height="20" Margin="79,199,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="label3" Content="Sekmadienis, Pirmadienis" HorizontalAlignment="Left" Height="30" Margin="80,239,0,0" VerticalAlignment="Top" Width="150"/>
        <ComboBox x:Name="comboBox3" HorizontalAlignment="Left" Height="20" Margin="80,269,0,0" VerticalAlignment="Top" Width="150"/>

    </Grid>
</Window>
